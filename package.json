{
  "name": "pciu-cgpa-calculator",
  "version": "1.0",
  "private": false,
  "scripts": {
    "postinstall": "npm run build",
    "start": "set NODE_ENV=production && node ./bin/www",
    "dev": "set NODE_ENV=development && nodemon ./bin/www",
    "build": "parcel build --no-source-maps public/index.html"
  },
  "nodemonConfig": {
    "ignore": ["public/*", "dist/*"]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "~4.16.1",
    "form-data": "^4.0.0",
    "node-html-parser": "^5.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "parcel": "^2.6.1"
  }
}
